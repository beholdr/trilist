{
  "name": "trilist",
  "version": "0.2.0",
  "description": "Web components for working with tree data: treeselect and treeview",
  "keywords": [
    "tree",
    "treeselect",
    "treeview",
    "svelte"
  ],
  "author": "Alexander Shabunevich",
  "repository": {
    "url": "git+https://github.com/beholdr/trilist.git",
    "type": "git"
  },
  "license": "MIT",
  "type": "module",
  "module": "./dist/trilist.js",
  "main": "./dist/trilist.umd.js",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/trilist.js",
      "require": "./dist/trilist.umd.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "version": "npm run build && npm test"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^2.0.0",
    "@tailwindcss/forms": "^0.5.6",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/svelte": "^4.0.4",
    "@testing-library/user-event": "^14.5.1",
    "@tsconfig/svelte": "^5.0.2",
    "@types/node": "^20.8.8",
    "@vitest/coverage-v8": "^0.34.6",
    "autoprefixer": "^10.4.16",
    "happy-dom": "^12.10.3",
    "postcss": "^8.4.31",
    "postcss-rem-to-responsive-pixel": "^6.0.0",
    "svelte": "^4.2.1",
    "svelte-check": "^3.5.2",
    "tailwindcss": "^3.3.3",
    "tailwindcss-animate": "^1.0.7",
    "tslib": "^2.4.1",
    "typescript": "^4.9.3",
    "vite": "^4.0.0",
    "vite-plugin-banner": "^0.7.1",
    "vite-plugin-dts": "^3.6.1",
    "vite-svg-loader": "^4.0.0",
    "vitest": "^0.34.6"
  }
}
