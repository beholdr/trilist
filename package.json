{
  "name": "trilist",
  "version": "0.4.0",
  "description": "Web components for working with tree data: treeselect and treeview",
  "keywords": [
    "tree",
    "treeselect",
    "treeview",
    "svelte"
  ],
  "author": "Alexander Shabunevich",
  "repository": {
    "url": "git+https://github.com/beholdr/trilist.git",
    "type": "git"
  },
  "license": "MIT",
  "module": "./dist/trilist.mjs",
  "main": "./dist/trilist.umd.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/trilist.mjs",
      "require": "./dist/trilist.umd.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "format": "prettier --write .",
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "version": "npm run build && npm test"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^3.1.1",
    "@tailwindcss/forms": "^0.5.6",
    "@tailwindcss/postcss": "^4.1.12",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/svelte": "^5.1.0",
    "@testing-library/user-event": "^14.5.1",
    "@tsconfig/svelte": "^5.0.2",
    "@types/node": "^20.8.8",
    "@vitest/coverage-v8": "^2.0.4",
    "happy-dom": "^14.12.3",
    "postcss": "^8.4.31",
    "postcss-rem-to-responsive-pixel": "^6.0.0",
    "prettier": "^3.3.2",
    "prettier-plugin-svelte": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "svelte": "^4.2.1",
    "svelte-check": "^3.5.2",
    "tailwindcss": "^4.1.12",
    "tslib": "^2.4.1",
    "tw-animate-css": "^1.3.7",
    "typescript": "^5.5.2",
    "vite": "^5.3.1",
    "vite-plugin-banner": "^0.7.1",
    "vite-plugin-dts": "^3.6.1",
    "vite-svg-loader": "^5.1.0",
    "vitest": "^2.0.4"
  }
}
